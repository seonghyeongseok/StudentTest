<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.skadl.studenttest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Login"/>
        <activity android:name=".Join"/>
        <activity android:name=".EnterRoom"/>
        <activity android:name=".Blindrace"
            android:screenOrientation="landscape"/>
        <activity android:name=".GroupMain"/>
        <activity android:name=".GradeRecord"/>
        <activity android:name=".Note"/>
        <activity android:name=".Retest"/>
        <activity android:name=".Feedback"/>
        <activity android:name=".MidResult"
            android:screenOrientation="landscape"/>
    </application>
<uses-permission android:name="android.permission.INTERNET" />
</manifest>